{
  "ImageUploadSettings": {
    "Product": "Products"
  },
  "AWS": {
    "Region": "us-east-1",
    "BucketName": "aspnetb11",
    "SqsUrl": "https://queue.amazonaws.com/847888492411/shahadat-queue",
    "UrlExpiryMinutes": 15
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Logs/worker-log-.log",
          "rollingInterval": "Day"
        }
      }
    ]
  }
}